<%s=5%><!--scale -->
<%cl=0.01%><!--chassis length -->
<%cr=0.25*s%><!--chassis radius -->
<%wr=0.0605*s%><!--wheel radius -->
<%wl=wr%><!--wheel length -->
<%mu=0%><!--friction coefficient on wheels range from 0 to 1 -->
<%slip=0%><!--slip coefficient range from 0 to 1 -->
<%m_ch=0.011%><!--chassis mass-->
<%m_wh=1%><!--mass of revolute wheels-->
<%m_cs=0.011%><!--mass of caster wheels-->
<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='m_4wrobot'>
	  <static>false</static>
	  <pose>0 0 0 0 0 0</pose>
	  <link name = 'chassis'>
		  <pose>0 0 <%=2*wr+cl/2%> 0 0 0</pose>
		  <visual name='visual'>
			  <geometry>
				  <cylinder>
					  <radius><%=cr%></radius>
					  <length><%=cl%></length>
				  </cylinder>
			  </geometry>
			  
		  </visual>
		  <visual name = 'visual1'>
			  <pose><%=cr*0.8%> 0 0 0 0 0</pose>
			  <geometry>
				  <cylinder>
					  <radius><%=cr/5%></radius>
					  <length><%=wl/5%></length>
				  </cylinder>
			  </geometry>
			  <material>
				<ambient>0 0.7 0 01</ambient>
				<diffuse>0 0.7 0 01</diffuse>
				<specular>0 0 0 0</specular>
				<emissive>0 0 0 0</emissive>
			  </material>
		  </visual>
		  <collision name='collision'>
			  <pose>0 0 0 0 0 0</pose>
			  <geometry>
				  <cylinder>
					  <radius><%=cr%></radius>
					  <length><%=cl%></length>
				  </cylinder>
			  </geometry>  
			<surface>
				<friction>
					<ode>
						<mu>0</mu>
						<mu2>0</mu2>
					</ode>
				</friction>
			</surface>
		  </collision>
	  </link>
	  
	<!--  <joint type = 'fixed' name = 'ground_clamp'>
		  <parent>world</parent>
